object C is{
  a : Integer := 1;
  b : Integer := 2;

  def init(z : Integer, e : Integer) is{
    this.a := z;
    this.b := e;
    this.a.toString().println();
    this.b.toString().println();
    return 4;
  }
}
{
  C.init(4,5);
  C.a.toString().println();
  C.b.toString().println();
}
